{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "react/no-unescaped-entities": 0,
        "react/prop-types": "off",
        "react-hooks/exhaustive-deps": 0,
        "react/react-in-jsx-scope": "off",
        "eslint-disable eqeqeq": 0
    }
}
